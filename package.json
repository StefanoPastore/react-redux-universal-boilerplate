{
  "name": "react-redux-universal-boilerplate",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "storybook": "NODE_ENV=development start-storybook -p 1818 -c ./playground/.storybook",
    "start": "if-env NODE_ENV=production && npm run start:prod || if-env NODE_ENV=staging && npm run start:stag || npm run start:dev",
    "start:dev": "rimraf public && rimraf build && npm-run-all --parallel build:dev development",
    "start:staging": "npm-run-all build:stag staging",
    "start:prod": "npm-run-all build:prod production",
    "development": "just-wait -p \"build/*.js\" && node ./build/server.bundle.js",
    "staging": "node ./build/server.bundle.js",
    "production": "node ./build/server.bundle.js",
    "build": "if-env NODE_ENV=production && npm run build:prod || if-env NODE_ENV=staging && npm run build:stag || npm run build:dev",
    "build:dev": "npm-run-all --parallel build:dev:server build:dev:client",
    "build:stag": "npm-run-all build:stag:server build:stag:client",
    "build:prod": "npm-run-all build:prod:server build:prod:client",
    "build:dev:client": "webpack --config webpack/client/config.dev.babel.js --progress --colors --display-error-details",
    "build:stag:client": "webpack --config webpack/client/config.stag.babel.js",
    "build:prod:client": "webpack --config webpack/client/config.prod.babel.js",
    "build:dev:server": "webpack --config webpack/server/config.dev.babel.js --progress --colors --display-error-details --watch",
    "build:stag:server": "webpack --config webpack/server/config.stag.babel.js",
    "build:prod:server": "webpack --config webpack/server/config.prod.babel.js",
    "pretest": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:StefanoPastore/react-redux-universal-boilerplate.git"
  },
  "author": "Creatiweb S.r.l.",
  "license": "ISC",
  "engine": "node 6.4.0",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "babel-polyfill": "^6.13.0",
    "classnames": "^2.2.5",
    "core-decorators": "^0.12.3",
    "express": "^4.14.0",
    "i18next": "^3.4.2",
    "i18next-browser-languagedetector": "^1.0.0",
    "i18next-resource-store-loader": "^0.1.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-i18next": "^1.10.0",
    "react-redux": "^4.4.5",
    "react-resolver": "^3.0.3",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "winston": "^2.2.0",
    "winston-graylog2": "^0.5.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.14.0",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-regenerator": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-jsx-control-statements": "^2.0.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "if-env": "^1.0.0",
    "jsx-control-statements": "^3.1.4",
    "just-wait": "^1.0.5",
    "npm-run-all": "^1.4.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "redux-devtools": "^3.3.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  }
}
